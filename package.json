{
  "name": "sb-web-node",
  "version": "1.0.0",
  "description": "SurgBook NodeJS API Server",
  "repository": "https://mteg.myds.me:7942/glory/sb-web-node.git",
  "author": "MTEG - 장성호",
  "license": "Custom",
  "main": "dist/index.js",
  "engines": {
    "node": "~6.9.1",
    "npm": ">=3.10.0"
  },
  "scripts": {
    "prestart": "npm run -s build",
    "start": "node dist/index.prod.js",
    "dev": "nodemon src/index.dev.js --exec \"node -r dotenv/config -r babel-register\"",
    "clean": "rimraf dist",
    "build": "npm run clean && mkdir dist && babel src -s -D -d dist",
    "test": "jest --watch",
    "lint": "esw -w src test"
  },
  "keywords": [],
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "body-parser": "^1.18.2",
    "dateformat": "^3.0.3",
    "express": "^4.16.2",
    "iconv": "^2.3.2",
    "jsonwebtoken": "^8.4.0",
    "knex": "^0.15.2",
    "mkdirp": "^0.5.1",
    "morgan": "^1.9.0",
    "mysql": "^2.16.0",
    "mysql2": "^1.6.4",
    "nodemailer": "^4.7.0",
    "npm": "^6.5.0",
    "phpjs": "^1.3.2",
    "pug": "^2.0.0-beta11",
    "rimraf": "^2.6.2",
    "sb-multer": "git+https://mteg.myds.me:7942/glory/sb-multer.git",
    "socketio-sticky-session": "^0.4.3",
    "string-template": "^1.0.0",
    "to": "^0.2.9",
    "update": "^0.7.4",
    "xml2js": "^0.4.19",
    "nodejs-aes256": "^1.0.1",
    "base64-url": "^2.2.0",
    "fs-extra": "^7.0.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "babel-jest": "^21.2.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-require-context-hook": "^1.0.0",
    "babel-register": "^6.26.0",
    "dotenv": "^4.0.0",
    "eslint": "^4.12.1",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jest": "^21.3.2",
    "eslint-watch": "^3.1.3",
    "jest": "^23.6.0",
    "nodemon": "^1.12.1",
    "supertest": "^3.0.0",
    "swagger-ui-express": "^4.0.2",
    "swagger-jsdoc": "^3.2.6"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "transform-object-rest-spread",
      "transform-class-properties",
      "require-context-hook",
      [
        "module-resolver",
        {
          "alias": {
            "@": "./src"
          }
        }
      ]
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "plugins": [
      "import",
      "jest"
    ],
    "parserOptions": {
      "ecmaVersion": 2017,
      "sourceType": "module"
    },
    "env": {
      "node": true,
      "jest": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "rules": {
      "jest/no-focused-tests": 2,
      "jest/no-identical-title": 2
    }
  },
  "jest": {
    "testEnvironment": "node"
  }
}
