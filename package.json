{
  "name": "sb-web-node",
  "version": "2.0.0",
  "description": "SurgStory NodeJS API Server",
  "repository": "http://pms.mteg.co.kr/surgstory/sb-web-node.git",
  "author": "MTEG - 황우중",
  "license": "Custom",
  "main": "dist/index.js",
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0"
  },
  "scripts": {
    "prestart": "npm run -s build",
    "start": "node dist/index.prod.js",
    "local": "nodemon src/index.local.js --exec \"node -r dotenv/config -r babel-register\"",
    "dev": "nodemon src/index.dev.js --exec \"node -r dotenv/config -r babel-register\"",
    "devlocal": "nodemon src/index.local.js --exec \"node -r dotenv/config -r babel-register --inspect-brk\"",
    "devDebug": "nodemon src/index.dev.js --exec \"node -r dotenv/config -r babel-register --inspect-brk\"",
    "demon": "nodemon src/index.demon.js --exec \"node -r dotenv/config -r babel-register\"",
    "clean": "rimraf dist",
    "build": "npm run clean && mkdir dist && babel src -s -D -d dist",
    "lint": "esw -w src test"
  },
  "keywords": [],
  "dependencies": {
    "assert": "^1.4.1",
    "aws-sdk": "^2.680.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "base64-url": "^2.3.3",
    "body-parser": "^1.18.2",
    "canvas": "^2.6.1",
    "child-process": "^1.0.2",
    "dateformat": "^3.0.3",
    "express": "^4.16.2",
    "ffprobe": "^1.1.0",
    "fs-extra": "^7.0.1",
    "get-video-dimensions": "^1.0.0",
    "get-video-duration": "^1.0.3",
    "jsonpath": "^1.0.1",
    "jsonwebtoken": "^8.4.0",
    "knex": "^0.17.5",
    "lodash": "^4.17.15",
    "log4js": "^3.0.6",
    "mediainfo-parser": "^1.1.5",
    "mime-types": "^2.1.27",
    "mkdirp": "^0.5.5",
    "moment": "^2.26.0",
    "mongoose": "^5.9.15",
    "mongoose-plugin-autoinc": "^1.1.9",
    "morgan": "^1.10.0",
    "multer": "^1.4.1",
    "multiparty": "^4.2.1",
    "mysql": "^2.18.1",
    "mysql2": "^1.6.4",
    "natsort": "^2.0.0",
    "node-ffprobe": "^1.2.2",
    "node-schedule": "^1.3.2",
    "nodejs-aes256": "^1.0.1",
    "npm": "^6.14.5",
    "promise": "^8.1.0",
    "pug": "^2.0.0-beta11",
    "query-string": "^6.11.1",
    "request": "^2.88.2",
    "request-promise": "^4.2.5",
    "rimraf": "^2.6.2",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "ssh-exec": "^2.0.0",
    "string-template": "^1.0.0",
    "striptags": "^3.1.1",
    "to": "^0.2.9",
    "update": "^0.7.4",
    "url-parse": "^1.4.7"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "babel-jest": "^21.2.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-require-context-hook": "^1.0.0",
    "babel-register": "^6.26.0",
    "dotenv": "^4.0.0",
    "eslint": "^4.12.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^21.3.2",
    "eslint-watch": "^3.1.3",
    "nodemon": "^1.19.4",
    "supertest": "^3.0.0",
    "swagger-jsdoc": "^3.7.0",
    "swagger-ui-express": "^4.1.4"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "transform-object-rest-spread",
      "transform-class-properties",
      "require-context-hook",
      [
        "module-resolver",
        {
          "alias": {
            "@": "./src"
          }
        }
      ]
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "plugins": [
      "import"
    ],
    "parserOptions": {
      "ecmaVersion": 2017,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "rules": {
      "indent": [
        "error",
        2,
        {"SwitchCase": 1}
      ]
    }
  }
}
